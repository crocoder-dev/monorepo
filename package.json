    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-head": "^3.4.2",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "unist-util-visit": "^4.1.1",
    "vite": "^3.0.9",
    "@mdx-js/mdx": "^2.1.3",
    "@mdx-js/react": "^2.1.3"
  },
